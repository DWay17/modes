<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MODE SYSTEM "xmode.dtd">
<MODE>
	<PROPS>
		<PROPERTY NAME="lineComment" VALUE="--"/>
		<PROPERTY NAME="commentStart" VALUE="/*" />
		<PROPERTY NAME="commentEnd" VALUE="*/" />
		<PROPERTY NAME="wrap" VALUE="soft"/>
	</PROPS>
	<RULES DEFAULT="NULL" HIGHLIGHT_DIGITS="TRUE" IGNORE_CASE="FALSE">
		<SPAN TYPE="COMMENT1">
			<BEGIN>/*</BEGIN>
			<END>*/</END>
		</SPAN>
		<EOL_SPAN TYPE="COMMENT2">--</EOL_SPAN>
		<MARK_PREVIOUS TYPE="KEYWORD2" MATCH_TYPE="OPERATOR">=</MARK_PREVIOUS>
		<MARK_PREVIOUS TYPE="KEYWORD2" MATCH_TYPE="OPERATOR"> =</MARK_PREVIOUS>
		<MARK_FOLLOWING TYPE="KEYWORD2">$</MARK_FOLLOWING>
		<SPAN TYPE="LITERAL1">
			<BEGIN>'</BEGIN>
			<END>'</END>
		</SPAN>
		<MARK_PREVIOUS TYPE="FUNCTION" MATCH_TYPE="OPERATOR">(</MARK_PREVIOUS>
		<SEQ TYPE="OPERATOR">)</SEQ>
		<KEYWORDS>
			<KEYWORD3>REGISTER</KEYWORD3>
			<KEYWORD3>STORE</KEYWORD3>
			<KEYWORD4>AS</KEYWORD4>
			<KEYWORD4>BY</KEYWORD4>
			<KEYWORD4>COUNT</KEYWORD4>
			<KEYWORD4>DISTINCT</KEYWORD4>
			<KEYWORD4>FILTER</KEYWORD4>
			<KEYWORD4>FOREACH</KEYWORD4>
			<KEYWORD4>GENERATE</KEYWORD4>
			<KEYWORD4>GROUP</KEYWORD4>
			<KEYWORD4>INTO</KEYWORD4>
			<KEYWORD4>JAR</KEYWORD4>
			<KEYWORD4>JOIN</KEYWORD4>
			<KEYWORD4>LOAD</KEYWORD4>
			<KEYWORD4>OR</KEYWORD4>
			<KEYWORD4>ORDER</KEYWORD4>
			<KEYWORD4>USING</KEYWORD4>
			<OPERATOR>as</OPERATOR>
			<OPERATOR>::</OPERATOR>
		</KEYWORDS>
	</RULES>
</MODE>