<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MODE SYSTEM "xmode.dtd">
<MODE>
	<RULES HIGHLIGHT_DIGITS="FALSE" IGNORE_CASE="FALSE">
		<!-- 
		<SPAN AT_LINE_START="TRUE" DELEGATE="MSH" MATCH_TYPE="KEYWORD1">MSH</SPAN>
		<SEQ AT_LINE_START="TRUE" DELEGATE="MSH" TYPE="KEYWORD3">MSH</SEQ>		
		<SEQ AT_LINE_START="TRUE" DELEGATE="OTHER" TYPE="KEYWORD4">PID</SEQ>
		-->                                                                 
		<SEQ_REGEXP AT_WHITESPACE_END="TRUE" TYPE="KEYWORD1">^(MSH|PID|PV1|PRB|ROL|ROL|OBX|GOL|ROL)</SEQ_REGEXP>
		<KEYWORDS>
			<MARKUP>|</MARKUP>          
			<MARKUP>^</MARKUP>                  
			<MARKUP>~</MARKUP>
			<MARKUP>\</MARKUP>
			<MARKUP>&amp;</MARKUP>
		</KEYWORDS>
	</RULES>
	<RULES SET="MSH">
		<KEYWORDS>
			<MARKUP>|</MARKUP>
			<MARKUP>^</MARKUP>
			<MARKUP>~</MARKUP>
			<MARKUP>\</MARKUP>
			<MARKUP>&amp;</MARKUP>
		</KEYWORDS>
	</RULES>
	<RULES SET="OTHER">
		<KEYWORDS>
			<MARKUP>|</MARKUP>
			<MARKUP>^</MARKUP>
			<MARKUP>~</MARKUP>
			<MARKUP>\</MARKUP>
			<MARKUP>&amp;</MARKUP>
		</KEYWORDS>
	</RULES>
</MODE>

