<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MODE SYSTEM "xmode.dtd">
<MODE>
	<PROPS>
		<PROPERTY NAME="lineComment" VALUE=">" />
		<PROPERTY NAME="noWordSep" VALUE="-_" />
	</PROPS>
	<RULES IGNORE_CASE="TRUE" HIGHLIGHT_DIGITS="FALSE" NO_WORD_SEP="-_:)">
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LITERAL4">&gt;&gt;&gt;&gt;</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LITERAL3">&gt;&gt;&gt;</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LITERAL2">&gt;&gt;</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LITERAL1">&gt;</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LITERAL1">| </EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LITERAL1">:</EOL_SPAN>
		<EOL_SPAN_REGEXP AT_LINE_START="TRUE" TYPE="KEYWORD3">(Subject|Betreff):</EOL_SPAN_REGEXP>
		<EOL_SPAN_REGEXP AT_LINE_START="TRUE" TYPE="KEYWORD1">(From|Sender|von):</EOL_SPAN_REGEXP>
		<EOL_SPAN_REGEXP AT_LINE_START="TRUE" TYPE="KEYWORD4">(To|An):</EOL_SPAN_REGEXP>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="KEYWORD4">Cc:</EOL_SPAN>
		<EOL_SPAN_REGEXP AT_LINE_START="TRUE" TYPE="KEYWORD2">X-[\w-]+:</EOL_SPAN_REGEXP>
		<EOL_SPAN_REGEXP AT_LINE_START="TRUE" TYPE="COMMENT4">List-[\w-]+:</EOL_SPAN_REGEXP>
		<EOL_SPAN_REGEXP AT_LINE_START="TRUE" TYPE="FUNCTION">Content-[\w-]+:</EOL_SPAN_REGEXP>
		<EOL_SPAN_REGEXP AT_LINE_START="TRUE" TYPE="COMMENT1">(Ort|Zeit):</EOL_SPAN_REGEXP>
		<SEQ TYPE="COMMENT2" DELEGATE="SIGNATURE" AT_LINE_START="TRUE">-- </SEQ>
		<SEQ TYPE="KEYWORD1">:-)</SEQ>
		<SEQ TYPE="KEYWORD1">:-(</SEQ>
		<SEQ TYPE="KEYWORD1">:)</SEQ>
		<SEQ TYPE="KEYWORD1">:(</SEQ>
		<SEQ TYPE="KEYWORD1">;-)</SEQ>
		<SEQ TYPE="KEYWORD1">;-(</SEQ>
		<SEQ TYPE="KEYWORD1">;)</SEQ>
		<SEQ TYPE="KEYWORD1">;(</SEQ>
		<!-- <MARK_PREVIOUS AT_LINE_START="TRUE" TYPE="LABEL">:</MARK_PREVIOUS> -->
		<EOL_SPAN_REGEXP AT_LINE_START="TRUE" TYPE="KEYWORD4">(Delivered-To|Errors-to|Reply-To|Return-Path):</EOL_SPAN_REGEXP>
		<EOL_SPAN_REGEXP AT_LINE_START="TRUE" TYPE="KEYWORD3">(Message-ID|References|In-Reply-To|Thread-\w+):</EOL_SPAN_REGEXP>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LABEL">Received:</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LABEL">Accept-Language:</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LABEL">Authentication-Results:</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LABEL">DKIM-Signature:</EOL_SPAN>
		<EOL_SPAN_REGEXP AT_LINE_START="TRUE" TYPE="LABEL">(Date|Gesendet):</EOL_SPAN_REGEXP>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LABEL">MIME-Version:</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LABEL">Nutzerkennzeichen:</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LABEL">Precedence:</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LABEL">Received:</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LABEL">Received-SPF:</EOL_SPAN>
		<EOL_SPAN AT_LINE_START="TRUE" TYPE="LABEL">User-Agent:</EOL_SPAN>
	</RULES>
	<RULES SET="SIGNATURE" DEFAULT="COMMENT3"/>
</MODE>
